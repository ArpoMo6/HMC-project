<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <GeneDef ParentName="GenePainBase">
    <defName>HMC_Pain_Reduced_Lite</defName>
    <label>reduced pain lite</label>
    <labelShortAdj>pain-dull-lite</labelShortAdj>
    <description>Carriers of this gene feel slightly less pain than other.</description>
    <iconPath>UI/Icons/Genes/Gene_PainReduced</iconPath>
	<displayCategory>HMC_Genes</displayCategory>
    <displayOrderInCategory>41</displayOrderInCategory>
    <painFactor>0.9</painFactor>
    <biostatMet>0</biostatMet>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>dull</symbol></li>
        <li><symbol>daze</symbol></li>
        <li><symbol>tough</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>

  <GeneDef ParentName="GenePainBase">
    <defName>HMC_Pain_Extra_Lite</defName>
    <label>extra pain lite</label>
    <labelShortAdj>wimpy-lite</labelShortAdj>
    <description>Carriers of this gene feel slightly more pain than other.</description>
    <iconPath>UI/Icons/Genes/Gene_ExtraPain</iconPath>
	<displayCategory>HMC_Genes</displayCategory>
    <displayOrderInCategory>42</displayOrderInCategory>
	<painFactor>1.1</painFactor>
    <biostatMet>0</biostatMet>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>wimpo</symbol></li>
        <li><symbol>whino</symbol></li>
        <li><symbol>cowar</symbol></li>
        <li><symbol>cry</symbol></li>
      </prefixSymbols>
      <suffixSymbols>
        <li><symbol>whiner</symbol></li>
        <li><symbol>wimp</symbol></li>
      </suffixSymbols>
    </symbolPack>
  </GeneDef>

  <GeneDef ParentName="GeneMoodBase">
    <defName>HMC_Mood_Realist</defName>
    <label>realist</label>
    <description>Carriers of this gene tend to have pragmatic views. They do not hold hopes and perceive the surrounding reality as it is.</description>
    <iconPath>UI/Icons/Genes/Gene_Mood_Realism</iconPath>
	<displayCategory>HMC_Genes</displayCategory>
    <displayOrderInCategory>43</displayOrderInCategory>
    <biostatMet>1</biostatMet>
	<biostatCpx>2</biostatCpx>
	<minAgeActive>15</minAgeActive>
    <forcedTraits>
      <li>
        <def>Pragmatist</def>
      </li>
    </forcedTraits>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>reali</symbol></li>
        <li><symbol>pragma</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>

  <GeneDef ParentName="GeneMoodBase">
    <defName>HMC_Mood_Bipolarity</defName>
    <label>bipolar</label>
    <description>Carriers of this gene are predisposed to extreme mood swings. When they happy, they can be very happy, but when they sad, they can become dangerously depressed.</description>
    <iconPath>UI/Icons/Genes/Gene_Mood_Bipolarity</iconPath>
	<displayCategory>HMC_Genes</displayCategory>
    <displayOrderInCategory>44</displayOrderInCategory>
    <biostatMet>4</biostatMet>
    <forcedTraits>
      <li>
        <def>Bipolar</def>
      </li>
    </forcedTraits>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>bipolar</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>

</Defs>