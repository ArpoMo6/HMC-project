	<!-- Horns -->

	<GeneDef ParentName="GeneHeadboneBase">
		<defName>HMC_HA_RamHorn</defName>
		<label>ram horns</label>
		<description>Carriers of this gene grow two big ram horns protruding from the forehead.</description>
		<iconPath>UI/Icons/Genes/Gene_RamHorn</iconPath>
		<renderNodeProperties>
		  <li>
			<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
			<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
			<texPath>Things/Pawn/Humanlike/HeadAttachments/RamHorn/RamHorn</texPath>
			<color>(128, 128, 128)</color>
			<parentTagDef>Head</parentTagDef>
			<!-- <layer>PostHeadgear</layer> -->
			<drawData>
			  <defaultData>
				<layer>80</layer>
			  </defaultData>
			  <dataNorth>
				<layer>10</layer>
			  </dataNorth>
			</drawData>
		  </li>
		</renderNodeProperties>
		<displayCategory>HMC_Genes</displayCategory>
		<displayOrderInCategory>106</displayOrderInCategory>
	</GeneDef>

	<!-- Noses -->

	<GeneDef ParentName="GeneNoseBase">
		<defName>HMC_HA_Nose_Orcoid</defName>
		<label>orcoid nose</label>
		<description>Carriers of this gene have orcoid-like nose.</description>
		<iconPath>UI/Icons/Genes/Gene_OrcNose</iconPath>
		<renderNodeProperties>
		  <li>
			<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
			<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
			<texPath>Things/Pawn/Humanlike/HeadAttachments/OrcNose/OrcNose</texPath>
			<narrowCrownHorizontalOffset>0.03</narrowCrownHorizontalOffset>
			<useSkinShader>true</useSkinShader>
			<useRottenColor>true</useRottenColor>
			<colorType>Skin</colorType>
			<parentTagDef>Head</parentTagDef>
			<rotDrawMode>Fresh, Rotting</rotDrawMode>
			<visibleFacing>
			  <li>East</li>
			  <li>South</li>
			  <li>West</li>
			</visibleFacing>
			<drawData>
			  <defaultData>
				<layer>66</layer>
			  </defaultData>
			</drawData>
		  </li>
		</renderNodeProperties>
		<missingGeneRomanceChanceFactor>0.6</missingGeneRomanceChanceFactor>
		<displayCategory>HMC_Genes</displayCategory>
		<displayOrderInCategory>107</displayOrderInCategory>
	</GeneDef>

</Defs>