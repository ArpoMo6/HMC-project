<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Ears -->

    <GeneDef Name="HMC_HeadAttachmentGeneBase" Abstract="True"><!-- 耳朵-->
        <biostatCpx>0</biostatCpx>
        <biostatMet>0</biostatMet>
        <randomChosen>true</randomChosen>
        <endogeneCategory>Ears</endogeneCategory>
        <displayCategory>HMC_Genes</displayCategory>
        <displayOrderInCategory>100</displayOrderInCategory>
        <iconColor>(0.75, 0.75, 0.75)</iconColor>
        <graphicData>
            <layer>PostHair</layer>
            <drawNorthAfterHair>true</drawNorthAfterHair>
            <useSkinShader>false</useSkinShader>
            <drawLoc>HeadTop</drawLoc>
        </graphicData>
        <!-- Collides with other genes with the same tags-->
        <exclusionTags>
            <li>Ears</li>
        </exclusionTags>
    </GeneDef>

    <GeneDef ParentName = "HMC_HeadAttachmentGeneBase">
        <defName>HMC_HA_DeerEar</defName>
        <label>deer ears</label>
        <description>Carriers of this gene grow deer-like ears, adults own antlers.</description>
        <iconPath>UI/Icons/Genes/Gene_DeerEar</iconPath>
		<displayOrderInCategory>101</displayOrderInCategory>
        <graphicData>
            <graphicPath>Things/Pawn/Humanlike/HeadAttachments/DeerEar/DeerEarAdultA</graphicPath>
            <colorType>Hair</colorType>
        </graphicData>
    </GeneDef>

    <GeneDef ParentName = "HMC_HeadAttachmentGeneBase">
        <defName>HMC_HA_FoxEar</defName>
        <label>fox ears</label>
        <description>Carriers of this gene grow fox-like ears.</description>
        <iconPath>UI/Icons/Genes/Gene_FoxEar</iconPath>
		<displayOrderInCategory>102</displayOrderInCategory>
        <graphicData>
            <graphicPaths>
                <li>Things/Pawn/Humanlike/HeadAttachments/FoxEar/FoxEarA</li>
                <li>Things/Pawn/Humanlike/HeadAttachments/FoxEar/FoxEarB</li>
                <li>Things/Pawn/Humanlike/HeadAttachments/FoxEar/FoxEarC</li>
                <li>Things/Pawn/Humanlike/HeadAttachments/FoxEar/FoxEarD</li>
                <li>Things/Pawn/Humanlike/HeadAttachments/FoxEar/FoxEarE</li>
                <li>Things/Pawn/Humanlike/HeadAttachments/FoxEar/FoxEarF</li>
            </graphicPaths>
            <colorType>Hair</colorType>
        </graphicData>
    </GeneDef>

    <GeneDef ParentName = "HMC_HeadAttachmentGeneBase">
        <defName>HMC_HA_HalfDemonEar</defName>
        <label>imp ears</label>
        <description>Carriers of this gene grow imp-like ears.</description>
        <iconPath>UI/Icons/Genes/Gene_HalfDemonEar</iconPath>
		<displayOrderInCategory>103</displayOrderInCategory>
        <graphicData>
            <colorType>Skin</colorType>
            <graphicPaths>
                <li>Things/Pawn/Humanlike/HeadAttachments/HalfDemonEar/HalfDemonEarA</li>
                <li>Things/Pawn/Humanlike/HeadAttachments/HalfDemonEar/HalfDemonEarB</li>
                <li>Things/Pawn/Humanlike/HeadAttachments/HalfDemonEar/HalfDemonEarC</li>
                <li>Things/Pawn/Humanlike/HeadAttachments/HalfDemonEar/HalfDemonEarD</li>
            </graphicPaths>
        </graphicData>
    </GeneDef>

    <GeneDef ParentName = "HMC_HeadAttachmentGeneBase">
        <defName>HMC_HA_FinEar</defName>
        <label>fin ears</label>
        <description>Carriers of this gene grow fish-like ears.</description>
        <iconPath>UI/Icons/Genes/Gene_FinEar</iconPath>
		<displayOrderInCategory>104</displayOrderInCategory>
        <graphicData>
            <colorType>Skin</colorType>
            <graphicPaths>
                <li>Things/Pawn/Humanlike/HeadAttachments/FinEar/FinEar</li>
            </graphicPaths>
        </graphicData>
    </GeneDef>

    <GeneDef ParentName = "HMC_HeadAttachmentGeneBase">
        <defName>HMC_HA_HalfElfEar</defName>
        <label>half elf ears</label>
        <description>Carriers of this gene grow short elf-like ears.</description>
        <iconPath>UI/Icons/Genes/Gene_HalfElfEar</iconPath>
		<displayOrderInCategory>105</displayOrderInCategory>
        <graphicData>
            <colorType>Skin</colorType>
            <graphicPaths>
                <li>Things/Pawn/Humanlike/HeadAttachments/HalfElfEar/HalfElfEar</li>
            </graphicPaths>
        </graphicData>
    </GeneDef>

	<!-- Horns -->

	<GeneDef ParentName="GeneHeadboneBase">
		<defName>HMC_HA_RamHorn</defName>
		<label>ram horns</label>
		<description>Carriers of this gene grow two big ram horns protruding from the forehead.</description>
		<iconPath>UI/Icons/Genes/Gene_RamHorn</iconPath>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/RamHorn/RamHorn</graphicPath>
			<color>(128, 128, 128)</color>
			<layer>PostHeadgear</layer>
		</graphicData>
		<displayCategory>HMC_Genes</displayCategory>
		<displayOrderInCategory>106</displayOrderInCategory>
	</GeneDef>

	<!-- Noses -->

	<GeneDef ParentName="GeneNoseBase">
		<defName>HMC_HA_Nose_Orcoid</defName>
		<label>orcoid nose</label>
		<description>Carriers of this gene have orcoid-like nose.</description>
		<iconPath>UI/Icons/Genes/Gene_OrcNose</iconPath>
		<graphicData>
		  <graphicPath>Things/Pawn/Humanlike/HeadAttachments/OrcNose/OrcNose</graphicPath>
		  <narrowCrownHorizontalOffset>0.03</narrowCrownHorizontalOffset>
		  <useSkinShader>true</useSkinShader>
		  <layer>PostHair</layer>
		</graphicData>
		<missingGeneRomanceChanceFactor>0.6</missingGeneRomanceChanceFactor>
		<displayCategory>HMC_Genes</displayCategory>
		<displayOrderInCategory>107</displayOrderInCategory>
	</GeneDef>

</Defs>