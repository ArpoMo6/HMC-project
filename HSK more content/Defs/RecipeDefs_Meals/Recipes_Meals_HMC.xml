<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>Cook_HMC_Chebupek</defName>
		<label>cook chebupek</label>
		<description>Prepares chebupek. Produces 2.</description>
		<jobString>roasting chebupek.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<workAmount>625</workAmount>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>0.3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Flour</li>
					</thingDefs>
				</filter>
				<count>0.06</count>
			</li>
		</ingredients>
		<products>
			<HMC_Chebupek>3</HMC_Chebupek>
		</products>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<thingDefs>
				<li>Flour</li>
			</thingDefs>
			<specialFiltersToDisallow>
				<li>AllowPlantFood</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<thingDefs>
				<li>Flour</li>
			</thingDefs>
			<disallowedThingDefs>
				<li>Meat_Megaspider</li>
				<li>Meat_Human</li>
				<li>InsectJelly</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
		<skillRequirements>
			<Cooking>5</Cooking>
		</skillRequirements>
		<workSkill>Cooking</workSkill>
		<recipeUsers>
			<li>TableOven</li>
			<li>ElectricOven</li>
		</recipeUsers>
		<researchPrerequisite>Bakery_C1</researchPrerequisite>
	</RecipeDef>

