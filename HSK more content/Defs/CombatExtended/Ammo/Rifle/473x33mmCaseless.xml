		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
			</categories>
			<thingDefs>
				<li>Powder</li>
				<li>Plastic</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_473x33mmCaseless_FMJ>96</Ammo_473x33mmCaseless_FMJ>
		</products>
		<workAmount>1800</workAmount>
		<skillRequirements>
